{
  "version": 2,
  "buildCommand": "npm run build:api",
  "installCommand": "npm install",
  "builds": [
    {
      "src": "apps/api/dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/api/dist/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
